{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "tags": [
    "-lintignore"
  ],
  "workspaces": {
    ".": {
      "entry": [
        "src/module.ts",
        "src/cli.ts",
        "src/clients/*.d.ts",
        "src/runtime/**/*.ts",
        "test/**/*.test.ts"
      ],
      "project": [
        "src/**/*.ts"
      ],
      "ignoreDependencies": [
        "@graphql-codegen/import-types-preset",
        "@graphql-codegen/introspection",
        "@shopify/api-codegen-preset",
        "@graphql-tools/graphql-tag-pluck"
      ]
    },
    "docs": {
      "entry": [
        "app/app.config.ts",
        "app/**/*.{vue,ts,css}",
        "graphql/**/*.ts",
        "content/**/*.md"
      ],
      "project": [
        "**/*.{ts,vue,css,md}"
      ],
      "ignoreDependencies": [
        "@nuxt/kit",
        "@nuxtjs/shopify",
        "better-sqlite3",
        "docus",
        "reka-ui",
        "vaul-vue"
      ]
    },
    "template": {
      "entry": [
        "app/**/*.{ts,vue,css}",
        "graphql/**/*.ts",
        "shared/**/*.ts",
        "i18n/locales/**",
        "content/**",
        "content.config.ts"
      ],
      "project": [
        "**/*.{ts,vue,css,md,json}"
      ],
      "ignoreDependencies": [
        "@nuxtjs/shopify",
        "vue-tsc"
      ]
    },
    "playgrounds/playground-v3": {
      "entry": [
        "app/**/*.{ts,vue}",
        "server/**/*.ts",
        "graphql/**/*.ts"
      ],
      "project": [
        "**/*.{ts,vue}"
      ]
    },
    "playgrounds/playground-v4": {
      "entry": [
        "app/**/*.{ts,vue}",
        "server/**/*.ts",
        "graphql/**/*.ts"
      ],
      "project": [
        "**/*.{ts,vue}"
      ]
    },
    "playgrounds/playground-v4-mock": {
      "entry": [
        "app/**/*.{ts,vue}",
        "server/**/*.ts",
        "graphql/**/*.ts"
      ],
      "project": [
        "**/*.{ts,vue}"
      ]
    }
  },
  "ignoreUnresolved": [
    "#i18n",
    "#shopify/.+",
    "#ui/.+"
  ]
}
